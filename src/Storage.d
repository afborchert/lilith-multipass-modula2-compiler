 EXPORT QUALIFIED ALLOCATE, DEALLOCATE, Allocate, Deallocate, SetMode;

 PROCEDURE ALLOCATE(VAR a:ADDRESS; n: CARDINAL);
 PROCEDURE Allocate(VAR a: ADDRESS; n: CARDINAL);

 PROCEDURE DEALLOCATE(VAR a: ADDRESS; n: CARDINAL);
 PROCEDURE Deallocate(VAR a: ADDRESS; n: CARDINAL);

 PROCEDURE SetMode(m: CARDINAL);

END Storage.
